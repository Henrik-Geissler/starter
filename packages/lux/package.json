{
  "name": "@lux/components",
  "version": "0.0.1",
  "description": "Lux component library - Experimental Stencil components",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/types/index.d.ts",
  "collection": "./dist/collection/collection-manifest.json",
  "collection:main": "./dist/collection/index.js",
  "unpkg": "./dist/lux/lux.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js"
    },
    "./react": {
      "types": "./dist/react/components.d.ts",
      "import": "./dist/react/components.ts"
    },
    "./angular": {
      "types": "./dist/angular/lux.module.d.ts",
      "import": "./dist/angular/lux.module.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs.js"
    },
    "./components": {
      "types": "./dist/types/components.d.ts",
      "import": "./dist/components/index.js"
    },
    "./components/lux-button.js": {
      "types": "./dist/components/lux-button.d.ts",
      "import": "./dist/components/lux-button.js"
    },
    "./components/lux-card.js": {
      "types": "./dist/components/lux-card.d.ts",
      "import": "./dist/components/lux-card.js"
    },
    "./components/lux-heading.js": {
      "types": "./dist/components/lux-heading.d.ts",
      "import": "./dist/components/lux-heading.js"
    },
    "./components/lux-icon.js": {
      "types": "./dist/components/lux-icon.d.ts",
      "import": "./dist/components/lux-icon.js"
    },
    "./components/lux-scope.js": {
      "types": "./dist/components/lux-scope.d.ts",
      "import": "./dist/components/lux-scope.js"
    },
    "./components/lux-tab.js": {
      "types": "./dist/components/lux-tab.d.ts",
      "import": "./dist/components/lux-tab.js"
    },
    "./components/lux-table.js": {
      "types": "./dist/components/lux-table.d.ts",
      "import": "./dist/components/lux-table.js"
    },
    "./components/lux-tabs.js": {
      "types": "./dist/components/lux-tabs.d.ts",
      "import": "./dist/components/lux-tabs.js"
    },
    "./components/lux-text.js": {
      "types": "./dist/components/lux-text.d.ts",
      "import": "./dist/components/lux-text.js"
    },
    "./dist/lux/lux.css": {
      "default": "./dist/lux/lux.css"
    }
  },
  "scripts": {
    "build": "stencil build --docs && npm run build:angular-module",
    "build:angular-module": "node scripts/generate-angular-module.js",
    "dev": "npm run dev:storybook",
    "dev:stencil": "stencil build --dev --watch --serve",
    "dev:storybook": "storybook dev -p 6006 --disable-telemetry",
    "test": "stencil test --spec",
    "test:watch": "stencil test --spec --watch",
    "test:coverage": "stencil test --spec --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "build:storybook": "storybook build --disable-telemetry"
  },
  "dependencies": {
    "@stencil/core": "^4.5.0",
    "@stencil/sass": "^3.0.0"
  },
  "peerDependencies": {
    "@fortawesome/fontawesome-free": "^6.5.0",
    "ag-grid-angular": "^31.0.0",
    "ag-grid-community": "^31.0.0",
    "ag-grid-react": "^31.0.0"
  },
  "devDependencies": {
    "@stencil/angular-output-target": "^1.2.0",
    "@stencil/react-output-target": "^1.3.0",
    "@storybook/addon-a11y": "^7.6.0",
    "@storybook/addon-essentials": "^7.6.0",
    "@storybook/addon-interactions": "^7.6.0",
    "@storybook/addon-links": "^7.6.0",
    "@storybook/blocks": "^7.6.0",
    "@storybook/html-webpack5": "^7.6.0",
    "@storybook/web-components": "^7.6.0",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-wc": "^1.4.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "puppeteer": "^20.0.0",
    "storybook": "^7.6.0",
    "typescript": "^5.3.3"
  }
}
